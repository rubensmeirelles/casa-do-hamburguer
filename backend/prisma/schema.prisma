generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name String
  email String @unique
  password String
  cep String
  admin Boolean @default(false)
}
